module pur\sa0\par\fi0\sb0
\{\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # Add purchasing info to vendor object.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
# EXTEND vendor::vendor\sa0\par\fi0\sb0
# \{\sa0\par\fi0\sb0
#   text    notes;\sa0\par\fi0\sb0
#       \sa0\par\fi0\sb0
#  # help en notes  "These notes will show up in the purchasing form"\sa0\par\fi0\sb0
#  #                "with an option to print on the purchase order.";\sa0\par\fi0\sb0
# \};\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # Recommended Vendor\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class use_vendor\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    vendor::vendor  *recommended;\sa0\par\fi0\sb0
    text             notes;\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # Purchasing Item\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class item : base_item::item\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char    buyer_code<8>;\sa0\par\fi0\sb0
    text    purch_text;\sa0\par\fi0\sb0
    char    purch_desc<100>;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    pur::use_vendor [] recommend_vendor;\sa0\par\fi0\sb0
   \sa0\par\fi0\sb0
    # help en buyer_code  "The buyer code is a reference to the buyer responsible"\sa0\par\fi0\sb0
    #                     "for purchasing this product.";\sa0\par\fi0\sb0
    # help en purch_text  "The purchasing text is text that appears on a line item"\sa0\par\fi0\sb0
    #                     "on the purchase order.  It would normally be used for"\sa0\par\fi0\sb0
    #                     "special notes.";\sa0\par\fi0\sb0
    # help en purch_desc  "The purchasing description replaces the normal item"\sa0\par\fi0\sb0
    #                     "description for purchase orders only.";\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
        \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # buyer - employees responsible for this purchase group\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class buyer\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char                buyer_group<8>;\sa0\par\fi0\sb0
    person::employee [] buyers;\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # this is standard text that can be referenced by id and copied to p.o.\sa0\par\fi0\sb0
  # ------------------------------------------------------------------------\sa0\par\fi0\sb0
  class text\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char      id<8>;     # is unique for id+buyer\sa0\par\fi0\sb0
    char      buyer<8>;  # buyer code\sa0\par\fi0\sb0
    text      text;      # for keeping standard text that is not by item\sa0\par\fi0\sb0
                         # normally for various special conditions\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # approval_list - a company wide list of all chains of approval.\sa0\par\fi0\sb0
  # ------------------------------------------------------------------------\sa0\par\fi0\sb0
  class approval_list\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char                name<25>;\sa0\par\fi0\sb0
    person::employee [] employee;\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # approval - the status of this po/req chain of approval\sa0\par\fi0\sb0
  # ------------------------------------------------------------------------\sa0\par\fi0\sb0
  class approval\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char              name<25>;  #list name\sa0\par\fi0\sb0
    person::employee *employee;\sa0\par\fi0\sb0
    boolean           approved     = false;\sa0\par\fi0\sb0
    boolean           not_approved = false;\sa0\par\fi0\sb0
    # pur::header      *what;      # what was approved (redundant)\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # status - overall status of this document\sa0\par\fi0\sb0
  #   req, approved req, po, approved po, sent po, closed po\sa0\par\fi0\sb0
  #   not approved req, not approved po.\sa0\par\fi0\sb0
  # ------------------------------------------------------------------------\sa0\par\fi0\sb0
  class status\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char name<25>;\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class detail\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    # pur::header     *header;         # redundant\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    int              line_number;      # the line on the po\sa0\par\fi0\sb0
    char             item_number<25>;  # the item ordered\sa0\par\fi0\sb0
    pur::item       *item;             # optional\sa0\par\fi0\sb0
    char             purch_desc<100>;\sa0\par\fi0\sb0
    text             purch_text;       # for free form notes by item.\sa0\par\fi0\sb0
    currency::money  price;\sa0\par\fi0\sb0
    unit::quantity   quantity;\sa0\par\fi0\sb0
    date             required_date;\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # header - for requisitions or purchase orders\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class header\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char    id<10>;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    vendor::vendor *vendor;\sa0\par\fi0\sb0
        \sa0\par\fi0\sb0
    char    payment_terms<25>;  # TODO should be pointer\sa0\par\fi0\sb0
    char    delivery_terms<25>; # TODO should be pointer\sa0\par\fi0\sb0
        \sa0\par\fi0\sb0
    date    date_placed;\sa0\par\fi0\sb0
    date    date_completed;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    pur::approval [] approval;\sa0\par\fi0\sb0
    pur::status     *status;\sa0\par\fi0\sb0
        \sa0\par\fi0\sb0
    text    po_notes;        # free form notes for the total purchase order\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    pur::detail [] detail;\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
    # help en po_notes  "These notes will show up in the purchasing form"\sa0\par\fi0\sb0
    #                   "with an option to print on the purchase order.";\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\};\sa0\par\fi0\sb0
