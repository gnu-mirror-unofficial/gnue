module inv\sa0\par\fi0\sb0
\{\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # type - Type of quantity\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class i_type\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char name<20>;\sa0\par\fi0\sb0
    char descr<70>;\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # stock_quantity - Quantity tracked.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class stock_quantity\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    unit::quantity   quantity; # various types of quantities\sa0\par\fi0\sb0
    inv::i_type     *i_type;   # on-hand, scrap, on order etc.\sa0\par\fi0\sb0
    whse::storage   *location; # in this one location.\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # stock_item -\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class stock_item : base_item::item\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
         \sa0\par\fi0\sb0
    inv::stock_quantity   [] quantity;   # various types of quantities\sa0\par\fi0\sb0
         \sa0\par\fi0\sb0
    unit::quantity    weight;\sa0\par\fi0\sb0
    unit::quantity    volume;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    char              msds_reference<100>;  # material data safety sheet reference\sa0\par\fi0\sb0
    text              inventory_notes;      # inventory handling note\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
\};\sa0\par\fi0\sb0
