module language \{\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # definition of languages.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class language\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char     code<8>;       # en = ISO for english\sa0\par\fi0\sb0
    char     dialect<8>;    # us = ISO for united states\sa0\par\fi0\sb0
    char     desc_en<25>;   # english description of language (required)\sa0\par\fi0\sb0
    char     desc_fr<25>;   # french language description of language\sa0\par\fi0\sb0
    char     desc_ot<25>;    # other language description of language\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # default system language dependent data for use by forms and reports\sa0\par\fi0\sb0
  # to assist the user.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class help\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    language::language * language;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    char  code<8>;            # code that matches language::language.code\sa0\par\fi0\sb0
    char  dialect<8>;         # dialect that matches language::language.dialect\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    char  name<255>;          # fully qualified module class field name.\sa0\par\fi0\sb0
                              #   TODO define/document format.\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
                              # other layers of help may be defined by\sa0\par\fi0\sb0
                              #   extending this class.\sa0\par\fi0\sb0
                              # base help has these fields = null\sa0\par\fi0\sb0
    char  company<15>;        # place holder until decided how it works in geas.\sa0\par\fi0\sb0
    char  usr_group<15>;     # if empty this change applies to all groups.\sa0\par\fi0\sb0
    char  usr<15>;           # if empty this change applies to all users.\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
                              # Help data\sa0\par\fi0\sb0
    char  help_text<2000>;    # full help text if the user clicks on help.\sa0\par\fi0\sb0
    char  help_keywords<100>; # keywords to help for searches.\sa0\par\fi0\sb0
    text  tool_tip;           # short help when the user's mouse passes over\sa0\par\fi0\sb0
                              #   the field.\sa0\par\fi0\sb0
    text  label;              # default label for this data, used for\sa0\par\fi0\sb0
                              #   both reports and forms.\sa0\par\fi0\sb0
    text  long_label;\sa0\par\fi0\sb0
    text  short_label;\sa0\par\fi0\sb0
    text  default_text;       # default text data for new fields.\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\};\sa0\par\fi0\sb0
