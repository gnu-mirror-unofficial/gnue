    char    street2<35>;\sa0\par\fi0\sb0
    char    street3<35>;\sa0\par\fi0\sb0
    char    city<35>;\sa0\par\fi0\sb0
    location::region  *region;      # state in the US\sa0\par\fi0\sb0
    location::country *country;\sa0\par\fi0\sb0
    char    postal<17>;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    # get the place as city, state, zip per local requirements\sa0\par\fi0\sb0
    # text get_place();\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
\};\sa0\par\fi0\sb0
