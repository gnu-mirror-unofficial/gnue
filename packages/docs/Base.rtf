module currency\sa0\par\fi0\sb0
\{\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # currency - definition of available currencies.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class currency\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char      code<8>;\sa0\par\fi0\sb0
    char      description<32>;\sa0\par\fi0\sb0
    int       default_decimals = 2;\sa0\par\fi0\sb0
      \sa0\par\fi0\sb0
    INDEX   (code);\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # money - TYPE definition of money.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  type money\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    int64      amount = 0;        # this integer has an implied decimal point\sa0\par\fi0\sb0
                                  # for example if amount = 456192\sa0\par\fi0\sb0
    int16      num_decimals = 2;  # and num_decimals = 3\sa0\par\fi0\sb0
                                  # then amount really has a value of 456.192\sa0\par\fi0\sb0
    currency::currency *currency;\sa0\par\fi0\sb0
#   char       currency<8>;\sa0\par\fi0\sb0
#   REFERENCE  currency_reference : currency::currency( code ) = this.currency;\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
                              # select the system configured default currency\sa0\par\fi0\sb0
    #INITIALISE code = currency_reference(code =\sa0\par\fi0\sb0
    #                                       $system.default_currency_code );\sa0\par\fi0\sb0
                              # read the number of decimals from the selected\sa0\par\fi0\sb0
                              #     currency object\sa0\par\fi0\sb0
    #INITIALISE num_decimals = code.default_decimals;\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # conversion_rule - rules for converting different currencies.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class conversion_rule\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    currency::currency  *to;\sa0\par\fi0\sb0
    float                numerator = 1.0;   # the conversion\sa0\par\fi0\sb0
    float                denominator = 1.0; # factor = numerator/denominator\sa0\par\fi0\sb0
    date                 date_begin;\sa0\par\fi0\sb0
    date                 date_end;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # conversion - convert between different currencies.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class conversion\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    currency::currency           *from;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    currency::conversion_rule [] rule;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    text convert_to( char to_currency, char from_value);\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
\};\sa0\par\fi0\sb0
