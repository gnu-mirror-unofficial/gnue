                             # address line\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    UNIQUE INDEX(code);\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # address - definition of physical addresses.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  type address\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
