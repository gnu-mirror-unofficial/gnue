    char   state<2>;\sa0\par\fi0\sb0
    float  latitude;\sa0\par\fi0\sb0
    float  longitude;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
    UNIQUE INDEX(zip_code);\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
  class region\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char  country<8>;\sa0\par\fi0\sb0
    char  code<8>;   # state/region abbrev\sa0\par\fi0\sb0
