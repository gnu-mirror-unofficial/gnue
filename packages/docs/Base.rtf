module base_acct\sa0\par\fi0\sb0
\{\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # transaction definition for transactions that can be generated from\sa0\par\fi0\sb0
  # the various subsystems.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class trans_def\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    char    name<25>      not null;  # long name\sa0\par\fi0\sb0
    char    abbrv<8>      not null;  # abbreviation used in place of code\sa0\par\fi0\sb0
                                     # for lookup by the subsystems.\sa0\par\fi0\sb0
    int16   code          not null;  # code using in transaction file to save\sa0\par\fi0\sb0
                                     # space\sa0\par\fi0\sb0
    char    account<15>   not null;  # General Ledger account number\sa0\par\fi0\sb0
    char    debit_credit  not null;  # C = credit, D = debit\sa0\par\fi0\sb0
    boolean active = true not null;  # is this transaction code usable\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # standard transaction data for all subsystems.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  type trans\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    int             code;         # the transaction code from base_acct::trans\sa0\par\fi0\sb0
    date            date;         # the transaction date\sa0\par\fi0\sb0
    char            account<10>;  # the general ledger account number\sa0\par\fi0\sb0
    boolean         exported;     # has this transaction been exported from sub-system\sa0\par\fi0\sb0
    currency::money amount;       # value of this transaction\sa0\par\fi0\sb0
    char            debit_credit; # is this a debit or credit transaction\sa0\par\fi0\sb0
    char            who<15>;      # login name creating this transaction\sa0\par\fi0\sb0
    char            extra1<15>;   # defined by the subsystem\sa0\par\fi0\sb0
    char            extra2<15>;   # defined by the subsystem\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
\};\sa0\par\fi0\sb0
