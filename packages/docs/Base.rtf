module unit\sa0\par\fi0\sb0
\{\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # unit - unit of measure\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class unit\sa0\par\fi0\sb0
  \{                       # keeps track of the different units\sa0\par\fi0\sb0
    char   code<8>;       #   of measure\sa0\par\fi0\sb0
    char   descr<35>;      # used in GNUe.\sa0\par\fi0\sb0
    int16  decimals = 0;  # default decimals for this unit\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # quantity - \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  type quantity\sa0\par\fi0\sb0
  \{                            # this definition allows us to abstract\sa0\par\fi0\sb0
                               # quantities as needed\sa0\par\fi0\sb0
                               # for example if value = 456192\sa0\par\fi0\sb0
    int64       value = 0;     # only integer quantities\sa0\par\fi0\sb0
    int16       decimals = 2;  # and decimals = 3\sa0\par\fi0\sb0
                               # then value is really 456.192\sa0\par\fi0\sb0
    unit::unit  *unit;         # pointer to unit\sa0\par\fi0\sb0
                               #   for example "kg"\sa0\par\fi0\sb0
                                 \sa0\par\fi0\sb0
    float get_float();           # \sa0\par\fi0\sb0
    text  get_text();            # for example "456.192 kg"\sa0\par\fi0\sb0
    void  set_float(float val);  #\sa0\par\fi0\sb0
  \};    \sa0\par\fi0\sb0
\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # conversion_unit - Storage for all of the conversion factors.  Only used\sa0\par\fi0\sb0
  # by unit::conversion.  Should never be used directly.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class conversion_unit\sa0\par\fi0\sb0
  \{\sa0\par\fi0\sb0
    unit::quantity  amount;\sa0\par\fi0\sb0
  \};\sa0\par\fi0\sb0
  \sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  # conversion - convert between different units.\sa0\par\fi0\sb0
  # -------------------------------------------------------------------------\sa0\par\fi0\sb0
  class conversion\sa0\par\fi0\sb0
  \{ \sa0\par\fi0\sb0
    unit::conversion_unit  [] units;\sa0\par\fi0\sb0
    \sa0\par\fi0\sb0
              # returns t or f if can or cannot do conversion\sa0\par\fi0\sb0
    char can_convert( text source_unit, text result_unit );\sa0\par\fi0\sb0
              # returns resulting amount\sa0\par\fi0\sb0
